    "prisma:push": "prisma db push --accept-data-loss",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2"
  },
  "devDependencies": {
    "prisma": "^6.16.2",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "typescript": "^5.6.3"
  }
}
