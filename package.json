{
  "name": "edutech",
  "private": true,
  "packageManager": "pnpm@8",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "db:generate": "cd packages/db && pnpm prisma generate",
    "db:push": "cd packages/db && pnpm prisma db push",
    "web": "cd apps/web && pnpm dev",
    "mobile": "cd apps/mobile && pnpm start"
  },
  "devDependencies": {
    "turbo": "latest",
    "typescript": "^5.5.0"
  }
}
